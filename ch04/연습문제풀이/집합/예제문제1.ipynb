   "metadata": {},
   "source": [
    "><small>1부터 45까지의 수 중에서 6개를 선택하여 로또 번호를 만드는 프로그램</small>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "96f9424f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{33, 2, 35, 37, 10, 31}\n",
      "[2, 10, 31, 33, 35, 37]\n"
     ]
    }
   ],
   "source": [
    "import random\n",
    "pick = set()\n",
    "while len(pick) < 6:\n",
    "    n = random.randint(1, 45)\n",
    "    if n not in pick:\n",
    "        pick.add(n)\n",
    "print(pick)\n",
    "print(sorted(pick))"
   ]
  }
