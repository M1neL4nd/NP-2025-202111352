   "source": [
    "><small>1부터 45까지의 수 중에서 6개를 선택하여 로또 번호를 만드는 프로그램</small>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a1b89f15",
   "metadata": {},
   "outputs": [],
   "source": [
    "import random\n",
    "pick = set()\n",
    "while len(pick) < 6:\n",
    "    n = random.randint(1,45) \n",
    "    if n not in pick:\n",
    "        pick.add(n) \n",
    "print(pick)\n",
    "print(sorted(pick)) "
   ]
  }
